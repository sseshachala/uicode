{
    "name": "laravie/authen",
    "description": "User Authentication Identifiers for Laravel",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Mior Muhammad Zaki",
            "email": "crynobone@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Laravie\\Authen\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Laravie\\Authen\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=7.2",
        "illuminate/auth": "^5.8 || ^6.0 || ^7.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.2",
        "orchestra/testbench": "^3.8 || ^4.0 || ^5.0"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.4-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev"
}
